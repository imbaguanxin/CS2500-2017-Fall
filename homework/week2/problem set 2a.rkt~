;Finger Exercise

;minus 2 function
(define (minus_2 num)
  (- num 2)
)

;calculate distance function
(define (distance t)
  (+ 1.5 (* 1.5 t))
)

;turn graph to function
(define (y-value x)
  (+ 10 (* -1 x))
)